{
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"@page/*": ["pages/*"],
			"@common/*": ["components/common/*"],
			"@components/*": ["components/*"],
			"@icons": ["assets/icons/index.ts"],
			"@icons/*": ["assets/icons/*"],
			"@styles/*": ["assets/styles/*"],
			"@images/*": ["assets/images/*"],
			"@data/*": ["assets/data/*"],
			"@assets/*": ["assets/*"],
			"@hooks/*": ["hooks/*"],
			"@utils/*": ["utils/*"],
			"@queries/*": ["services/queries/*"],
			"@api/*": ["services/api/*"],
			"#services/*": ["services/*"]
